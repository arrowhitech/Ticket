<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <!-- 1 ST -->

    <LinearLayout
        android:id="@+id/temp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bghighlight"
        android:orientation="vertical" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="1 St"
            android:textColor="#f47320"
            android:textSize="25dip"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:text="LEVEL"
                    android:textColor="#f47320"
                    android:textSize="20dip" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <EditText
                        android:id="@+id/ed1STLevelFeet"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:background="@xml/highlight_edittext"
                        android:hint="FEET"
                        android:imeOptions="actionNext"
                        android:inputType="number"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />

                    <EditText
                        android:id="@+id/ed1STLevelInches"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:background="@xml/highlight_edittext"
                        android:hint="INCHES"
                        android:imeOptions="actionNext"
                        android:inputType="number"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />

                    <EditText
                        android:id="@+id/ed1STLevel14In"
                        android:layout_width="150dp"
                        android:layout_height="50dp"
                        android:background="@xml/highlight_edittext"
                        android:hint="1/4 IN"
                        android:imeOptions="actionNext"
                        android:inputType="number"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:text="TEMP"
                    android:textColor="#f47320"
                    android:textSize="20dip" />

                <EditText
                    android:id="@+id/ed1STTemp"
                    android:layout_width="235dp"
                    android:layout_height="50dp"
                    android:background="@xml/highlight_edittext"
                    android:imeOptions="actionNext"
                    android:nextFocusDown="@+id/edother1"
                    android:paddingLeft="3dip" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:text="BBLS"
                    android:textColor="#f47320"
                    android:textSize="20dip" />

                <EditText
                    android:id="@+id/ed1STBBLS"
                    android:layout_width="235dp"
                    android:layout_height="50dp"
                    android:background="@xml/highlight_edittext"
                    android:imeOptions="actionNext"
                    android:inputType="number"
                    android:nextFocusDown="@+id/edother1"
                    android:paddingLeft="3dip" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="vertical" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="S W"
                android:textColor="#f47320"
                android:textSize="20dip" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <EditText
                    android:id="@+id/ed1STSWFeet"
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:background="@xml/highlight_edittext"
                    android:hint="FEET"
                    android:imeOptions="actionNext"
                    android:inputType="number"
                    android:nextFocusDown="@+id/edother1"
                    android:paddingLeft="3dip" />

                <EditText
                    android:id="@+id/ed1STSWInches"
                    android:layout_width="150dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="20dp"
                    android:background="@xml/highlight_edittext"
                    android:hint="INCHES"
                    android:imeOptions="actionNext"
                    android:inputType="number"
                    android:nextFocusDown="@+id/edother1"
                    android:paddingLeft="3dip" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
