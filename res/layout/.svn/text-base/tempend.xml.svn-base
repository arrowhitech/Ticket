<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/llPlainMarketing"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    android:layout_marginTop="10dp"
    android:paddingLeft="10dip"
    android:paddingRight="10dip" >
    <!-- UNIT TYPE 1 -->
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bghighlight"
        android:orientation="vertical" >
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="NO.UNITS/TYPE"
                        android:textColor="#f47320"
                        android:textSize="20dip" />
                    <EditText
                        android:id="@+id/edTicketNo_UnitType1"
                        android:layout_width="200dp"
                        android:layout_height="50dp"
                        android:background="@xml/highlight_edittext"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:orientation="vertical" >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="HM"
                        android:textColor="#f47320"
                        android:textSize="20dip" />
                    <EditText
                        android:id="@+id/edTicketHM1"
                        android:layout_width="80dp"
                        android:layout_height="50dp"
                        android:background="@xml/highlight_edittext"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="10dp" >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="PROPER SHIPPING NAME,HAZARD CLASS,UN/NA NUMBER"
                android:textColor="#f47320"
                android:textSize="20dip" />
            <EditText
                android:id="@+id/edTicketProper1"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@xml/highlight_edittext"
                android:imeOptions="actionNext"
                android:nextFocusDown="@+id/edother1"
                android:paddingLeft="3dip" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="10dp" >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="NET BARRELS"
                android:textColor="#f47320"
                android:textSize="20dip" />
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >
                <EditText
                    android:id="@+id/edTicketNetBarrels1"
                    android:layout_width="200dp"
                    android:layout_height="50dp"
                    android:background="@xml/highlight_edittext"
                    android:imeOptions="actionNext"
                    android:nextFocusDown="@+id/edother1"
                    android:paddingLeft="3dip" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <include
        layout="@layout/tank_refusal"/>
