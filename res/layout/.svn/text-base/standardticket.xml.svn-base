<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/llPlainMarketing"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    android:paddingLeft="10dip"
    android:paddingRight="10dip" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none" >

        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <!-- Header -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="RUN TICKET"
                        android:textColor="#779a3d"
                        android:textSize="35dip"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="TICKET NUMBER"
                        android:textColor="#f47320"
                        android:textSize="20dip"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/edTicketNumber"
                        android:layout_width="200dp"
                        android:layout_height="50dp"
                        android:background="@xml/highlight_edittext"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="TICKET DATE"
                        android:textColor="#f47320"
                        android:textSize="20dip" />

                    <EditText
                        android:id="@+id/edTicketDate"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:background="@xml/highlight_edittext"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />
                </LinearLayout>
                
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="CUSTOMER"
                        android:textColor="#f47320"
                        android:textSize="20dip" />

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <EditText
                            android:id="@+id/edtCustomer"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="@null"
                            android:focusable="false"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />

                        <ImageButton
                            android:id="@+id/imgCustomer"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="right"
                            android:layout_marginBottom="10dp"
                            android:layout_marginLeft="210dp"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/nut_tam_giac"
                            android:src="@null" />

                        <Spinner
                            android:id="@+id/spCustomer"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:prompt="@string/promt" />
                    </FrameLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="OPERATOR"
                        android:textColor="#f47320"
                        android:textSize="20dip" />

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <EditText
                            android:id="@+id/edticketOperator"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="@null"
                            android:focusable="false"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />

                        <ImageButton
                            android:id="@+id/imgOperation"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_gravity="right"
                            android:layout_marginBottom="10dp"
                            android:layout_marginLeft="210dp"
                            android:layout_marginRight="10dp"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/nut_tam_giac"
                            android:src="@null" />

                        <Spinner
                            android:id="@+id/spOperation"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:prompt="@string/promt" />
                    </FrameLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="LEASE NAME"
                        android:textColor="#f47320"
                        android:textSize="20dip" />

                    <EditText
                        android:id="@+id/edTicketLeaseName"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:background="@xml/highlight_edittext"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="STATE"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edTicketState"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="320dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="COUNTY"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edTicketCountry"
