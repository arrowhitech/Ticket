                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edLocationTowship1"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="vertical" >

                        <EditText
                            android:id="@+id/edLocationTowship2"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_marginTop="25dip"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <EditText
                            android:id="@+id/edLocationTowship3"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_marginTop="25dip"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="DEC"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edLocationTowDec"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="DIR N/S"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edLocationTowDirNS"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal"
                    android:visibility="visible" >

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="RANGE"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edLocationRange1"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="vertical" >

                        <EditText
                            android:id="@+id/edLocationRange2"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_marginTop="25dip"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <EditText
                            android:id="@+id/edLocationRange3"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_marginTop="25dip"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="DEC"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edLocationRanDec"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="DIR N/S"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edLocationRanDirNS"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="100dp"
                    android:orientation="horizontal"
                    android:visibility="visible" >

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="MERIDIAN"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edLocationMeridian1"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" >

                            <requestFocus />
                        </EditText>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical" >

                        <EditText
                            android:id="@+id/edLocationMeridian2"
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_marginTop="25dip"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <!-- Flacno -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="vertical"
                android:paddingBottom="20dp" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="FLAC NO/LEASE NO"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edTicketFlacNo"
                            android:layout_width="200dp"
                            android:layout_height="50dp"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edother1"
                            android:paddingLeft="3dip" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:text="DISTRICT NO"
                            android:textColor="#f47320"
                            android:textSize="20dip" />

                        <EditText
                            android:id="@+id/edTicketDistrictNo"
