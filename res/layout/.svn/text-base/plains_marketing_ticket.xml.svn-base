<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/llPlainMarketing"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    android:paddingLeft="10dip"
    android:paddingRight="10dip" >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="RUN TICKET - Plains Marketing"
        android:textColor="#779a3d"
        android:textSize="30dip"
        android:textStyle="bold" />

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none" >

        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <!-- Header -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal" >

                    <EditText
                        android:id="@+id/edPetroleum1"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:background="@xml/highlight_edittext"
                        android:gravity="center"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edPetroleum2" >
                    </EditText>

                    <EditText
                        android:id="@+id/edPetroleum2"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="15dp"
                        android:background="@xml/highlight_edittext"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edother1"
                        android:paddingLeft="3dip" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal" >

                    <EditText
                        android:id="@+id/edother1"
                        android:layout_width="100dp"
                        android:layout_height="fill_parent"
                        android:background="@xml/highlight_edittext"
                        android:gravity="center"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edother2" />

                    <EditText
                        android:id="@+id/edother2"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="15dp"
                        android:background="@xml/highlight_edittext"
                        android:hint="OTHER"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edPlainTicketNumber"
                        android:paddingLeft="5dip" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical" >

                    <FrameLayout
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dip"
                            android:layout_marginTop="5dip"
                            android:text="TICKET NUMBER"
                            android:textSize="15dip" />

                        <EditText
                            android:id="@+id/edPlainTicketNumber"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:maxLength="7"
                            android:nextFocusDown="@+id/edPlainOperator"
                            android:paddingLeft="5dip"
                            android:paddingTop="20dip" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="3dip"
                        android:layout_weight="1"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dip"
                            android:layout_marginTop="5dip"
                            android:text="OPERATOR"
                            android:textSize="15dip" />

                        <EditText
                            android:id="@+id/edPlainOperator"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edPlainLease"
                            android:paddingLeft="5dip"
                            android:paddingTop="20dip" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="5dip"
                        android:layout_weight="1"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dip"
                            android:layout_marginTop="5dip"
                            android:text="LEASE NAME"
                            android:textSize="15dip" />

                        <EditText
                            android:id="@+id/edPlainLease"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edPlainField"
                            android:paddingLeft="5dip"
                            android:paddingTop="20dip" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        android:layout_marginTop="5dip"
                        android:layout_weight="1" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dip"
                            android:layout_marginTop="5dip"
                            android:text="FIELD"
                            android:textSize="15dip" />

                        <EditText
                            android:id="@+id/edPlainField"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@xml/highlight_edittext"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edPlainsLeaseNumber1"
                            android:paddingLeft="5dip"
                            android:paddingTop="20dip" />
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>

            <!-- Body -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="80dp"
                    android:layout_marginBottom="20dp"
                    android:orientation="horizontal" >

                    <EditText
                        android:id="@+id/edPlainsLeaseNumber1"
                        android:layout_width="70dp"
                        android:layout_height="67dp"
                        android:layout_gravity="bottom"
                        android:background="@xml/highlight_edittext"
                        android:gravity="center"
                        android:imeOptions="actionNext"
                        android:nextFocusDown="@+id/edPlainLeaseNumber2" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="20dp"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:background="@xml/rectange"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:background="@drawable/bg_cam"
                                android:gravity="center"
                                android:text="LEASE NUMBER"
                                android:textColor="#fff" />

                            <EditText
                                android:id="@+id/edPlainLeaseNumber2"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:background="@xml/highlight_edittext"
                                android:gravity="center"
                                android:imeOptions="actionNext"
                                android:maxLength="6"
                                android:nextFocusDown="@+id/edPlainsTankNumber" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1" >
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="80dp"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginRight="20dp"
                        android:layout_weight="2"
                        android:background="@xml/rectange"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_cam"
                            android:gravity="center"
                            android:text="TANK NUMBER"
                            android:textColor="#fff" />

                        <EditText
                            android:id="@+id/edPlainsTankNumber"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@xml/highlight_edittext"
                            android:gravity="center"
                            android:imeOptions="actionNext"
                            android:nextFocusDown="@+id/edPlainsTankSize" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="30dp"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:background="@xml/rectange"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bg_cam"
                                android:gravity="center"
                                android:text="TANK SIZE"
                                android:textColor="#fff" />

                            <EditText
                                android:id="@+id/edPlainsTankSize"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:background="@xml/highlight_edittext"
                                android:gravity="center"
                                android:imeOptions="actionNext"
                                android:nextFocusDown="@+id/edPlainsControlNumber" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1" >
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="80dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal"
                    android:visibility="visible" >

                    <TextView
                        android:layout_width="65dp"
                        android:layout_height="52dp"
                        android:layout_gravity="bottom"
                        android:background="@drawable/bg_cam"
                        android:gravity="center"
                        android:text="Ticket date"
                        android:textColor="#fff" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:background="@xml/rectange"
                            android:orientation="vertical" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bg_cam"
                                android:gravity="center_vertical" >

                                <TextView
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="DATE"
                                    android:textColor="#fff" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <Button
                                    android:id="@+id/edPlainsDate"
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:layout_weight="1"
                                    android:background="@xml/highlight_edittext"
                                    android:gravity="center" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="30dp"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:background="@xml/rectange"
                                android:orientation="vertical" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_cam"
                                    android:gravity="center"
                                    android:text="CONTROL NUMBER"
                                    android:textColor="#fff" />

                                <EditText
                                    android:id="@+id/edPlainsControlNumber"
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:background="@xml/highlight_edittext"
                                    android:gravity="center"
                                    android:imeOptions="actionNext"
                                    android:nextFocusDown="@+id/edPlains_first_gauge_feet" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1" >
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="100dp"
                    android:orientation="horizontal"
                    android:visibility="visible" >

                    <TextView
                        android:layout_width="65dp"
                        android:layout_height="65dp"
                        android:layout_gravity="bottom"
                        android:background="@drawable/bg_cam"
                        android:gravity="center"
                        android:text="FIRST"
                        android:textColor="#fff" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:background="@xml/rectange"
                                android:orientation="vertical" >

                                <LinearLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_cam"
                                    android:orientation="vertical" >

                                    <TextView
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="TANK GAUGE OIL LEVEL"
                                        android:textColor="#fff" />

                                    <LinearLayout
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal" >

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="FEET"
                                            android:textColor="#fff" />

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="INCHES"
                                            android:textColor="#fff" />

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="FR.IN"
                                            android:textColor="#fff" />
                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="fill_parent"
                                    android:layout_weight="1"
                                    android:orientation="horizontal" >

                                    <EditText
                                        android:id="@+id/edPlains_first_gauge_feet"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:background="@xml/highlight_edittext"
                                        android:gravity="center"
                                        android:imeOptions="actionNext"
                                        android:nextFocusDown="@+id/edPlains_first_gauge_inches" />

                                    <EditText
                                        android:id="@+id/edPlains_first_gauge_inches"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:background="@xml/highlight_edittext"
                                        android:gravity="center"
                                        android:imeOptions="actionNext"
                                        android:nextFocusDown="@+id/edPlains_first_gauge_frin" />

                                    <EditText
                                        android:id="@+id/edPlains_first_gauge_frin"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:background="@xml/highlight_edittext"
                                        android:gravity="center"
                                        android:imeOptions="actionNext"
                                        android:nextFocusDown="@+id/edPlains_first_Tank" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_marginLeft="30dp"
                                android:layout_weight="2"
                                android:background="@xml/rectange"
                                android:orientation="vertical" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_cam"
                                    android:gravity="center"
                                    android:text="TANK TEMP"
                                    android:textColor="#fff" />

                                <EditText
                                    android:id="@+id/edPlains_first_Tank"
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:background="@xml/highlight_edittext"
                                    android:gravity="center"
                                    android:imeOptions="actionNext"
                                    android:nextFocusDown="@+id/edPlainBswFeet" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_marginLeft="30dp"
                                android:layout_weight="1"
                                android:background="@xml/rectange"
                                android:orientation="vertical" >

                                <LinearLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_cam"
                                    android:orientation="vertical" >

                                    <TextView
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="BS&amp;W LEVEL"
                                        android:textColor="#fff" />

                                    <LinearLayout
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content" >

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="FEET"
                                            android:textColor="#fff" />

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="INCHES"
                                            android:textColor="#fff" />

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="FR.IN"
                                            android:textColor="#fff" />
                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:orientation="horizontal" >

                                    <EditText
                                        android:id="@+id/edPlainBswFeet"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:background="@xml/highlight_edittext"
                                        android:gravity="center"
                                        android:imeOptions="actionNext"
                                        android:nextFocusDown="@+id/edPlainsBswInches" />

                                    <EditText
                                        android:id="@+id/edPlainsBswInches"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:background="@xml/highlight_edittext"
                                        android:gravity="center"
                                        android:imeOptions="actionNext"
                                        android:nextFocusDown="@+id/edPlainsBswFrin" />

                                    <EditText
                                        android:id="@+id/edPlainsBswFrin"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:background="@xml/highlight_edittext"
                                        android:gravity="center"
                                        android:imeOptions="actionNext"
                                        android:nextFocusDown="@+id/edPlains_secon_gauge_feet" />
                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="3" >
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="100dp"
                    android:layout_marginBottom="20dp"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal"
                    android:visibility="visible" >

                    <TextView
                        android:layout_width="65dp"
                        android:layout_height="65dp"
                        android:layout_gravity="bottom"
                        android:background="@drawable/bg_cam"
                        android:gravity="center"
                        android:text="SECON"
                        android:textColor="#fff" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:background="@xml/rectange"
                                android:orientation="vertical" >

                                <LinearLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@drawable/bg_cam"
                                    android:orientation="vertical" >

                                    <LinearLayout
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1" >

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="TANK GAUGE OIL LEVEL"
                                            android:textColor="#fff" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="fill_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:orientation="horizontal" >

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="FEET"
                                            android:textColor="#fff" />

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="INCHES"
                                            android:textColor="#fff" />

                                        <TextView
                                            android:layout_width="fill_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="center"
                                            android:text="FR.IN"
                                            android:textColor="#fff" />
                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:orientation="horizontal" >

                                    <EditText
                                        android:id="@+id/edPlains_secon_gauge_feet"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:background="@xml/highlight_edittext"
                                        android:gravity="center"
                                        android:imeOptions="actionNext"
                                        android:nextFocusDown="@+id/edPlains_secon_gauge_inches" />

                                    <EditText
                                        android:id="@+id/edPlains_secon_gauge_inches"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
                                        android:background="@xml/highlight_edittext"
                                        android:gravity="center"
                                        android:imeOptions="actionNext"
                                        android:nextFocusDown="@+id/edPlains_secon_gauge_frin" />

                                    <EditText
                                        android:id="@+id/edPlains_secon_gauge_frin"
                                        android:layout_width="fill_parent"
                                        android:layout_height="fill_parent"
                                        android:layout_weight="1"
