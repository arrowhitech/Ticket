                            android:layout_marginRight="20dp"
                            android:layout_weight="1"
                            android:background="@xml/rectange"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bg_cam"
                                android:gravity="center"
                                android:text="METER READING"
                                android:textColor="#fff" />

                            <EditText
                                android:id="@+id/edStartMeterReading"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:background="@xml/highlight_edittext"
                                android:gravity="center"
                                android:imeOptions="actionNext"
                                android:nextFocusDown="@+id/edOnserialNumber" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="40dp"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="60dp"
                            android:layout_height="fill_parent"
                            android:layout_marginTop="20dp"
                            android:background="@drawable/bg_cam"
                            android:gravity="center"
                            android:text="ON"
                            android:textColor="#fff" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:background="@xml/rectange"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/bg_cam"
                                android:gravity="center"
                                android:text="SERIAL NUMBERS"
                                android:textColor="#fff" />

                            <EditText
                                android:id="@+id/edOnserialNumber"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:background="@xml/highlight_edittext"
                                android:gravity="center"
                                android:imeOptions="actionNext"
                                android:nextFocusDown="@+id/edRemark" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <!-- Botton -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dip"
                        android:layout_marginTop="5dip"
                        android:text="REMARKS"
                        android:textSize="15dip" />

                    <EditText
                        android:id="@+id/edRemark"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@xml/highlight_edittext"
                        android:gravity="top"
                        android:imeOptions="actionNext"
                        android:inputType="textMultiLine"
                        android:lines="5"
                        android:nextFocusDown="@+id/edOpeningTime"
                        android:paddingLeft="5dip"
                        android:paddingTop="20dip" >
                    </EditText>
                </FrameLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="120dp"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:orientation="vertical" >

                        <FrameLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dip"
                                android:layout_marginTop="5dip"
                                android:text="OPENING TIME"
                                android:textSize="15dip" />

                            <EditText
                                android:id="@+id/edOpeningTime"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:background="@xml/highlight_edittext"
                                android:imeOptions="actionNext"
                                android:nextFocusDown="@+id/edDriverGuager"
                                android:paddingLeft="5dip"
                                android:paddingTop="20dip" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dip"
                                android:layout_marginTop="5dip"
                                android:text="CLOSING DATE"
                                android:textSize="15dip" />

                            <EditText
                                android:id="@+id/edCloseDate"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:background="@xml/highlight_edittext"
                                android:imeOptions="actionNext"
                                android:nextFocusDown="@+id/edOparatorWitness"
                                android:paddingLeft="5dip"
                                android:paddingTop="20dip" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dip"
                                android:layout_marginTop="5dip"
                                android:text="CLOSING TIME"
                                android:textSize="15dip" />

                            <EditText
                                android:id="@+id/edCloseTime"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:background="@xml/highlight_edittext"
                                android:imeOptions="actionNext"
                                android:nextFocusDown="@+id/edCloseDriverGuager"
                                android:paddingLeft="5dip"
                                android:paddingTop="20dip" />
                        </FrameLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="5"
                            android:background="@xml/rectange"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:background="@xml/rectange"
                                android:gravity="center_horizontal"
                                android:text="OPEN" />

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:background="@xml/rectange"
                                android:gravity="center_horizontal"
                                android:text="CLOSE" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical" >

                            <FrameLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dip"
                                    android:layout_marginTop="5dip"
                                    android:text="DRIVER/GUAGER"
                                    android:textSize="15dip" />

                                <EditText
                                    android:id="@+id/edDriverGuager"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@xml/highlight_edittext"
                                    android:imeOptions="actionNext"
                                    android:nextFocusDown="@+id/edCloseDate"
                                    android:paddingLeft="5dip"
                                    android:paddingTop="20dip" />
                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dip"
                                    android:layout_marginTop="5dip"
                                    android:text="OPARATORS WITNESS"
                                    android:textSize="15dip" />

                                <EditText
                                    android:id="@+id/edOparatorWitness"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@xml/highlight_edittext"
                                    android:hint=""
                                    android:imeOptions="actionNext"
                                    android:nextFocusDown="@+id/edCloseTime"
                                    android:paddingLeft="5dip"
                                    android:paddingTop="20dip" />
                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dip"
                                    android:layout_marginTop="5dip"
                                    android:text="DRIVER/GUAGER"
                                    android:textSize="15dip" />

                                <EditText
                                    android:id="@+id/edCloseDriverGuager"
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:background="@xml/highlight_edittext"
                                    android:imeOptions="actionNext"
                                    android:nextFocusDown="@+id/edCloseOparator"
                                    android:paddingLeft="5dip"
                                    android:paddingTop="20dip" />
                            </FrameLayout>

                            <FrameLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dip"
                                    android:layout_marginTop="5dip"
                                    android:text="OPARATORS WITNESS"
                                    android:textSize="15dip" />

                                <EditText
                                    android:id="@+id/edCloseOparator"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@xml/highlight_edittext"
                                    android:hint=""
                                    android:paddingLeft="5dip"
                                    android:paddingTop="20dip"
                                    android:singleLine="true" />
                            </FrameLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="vertical" >

                <Button
                    android:id="@+id/btPlainsSave"
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="20dp"
                    android:background="@xml/custom_button_orange"
                    android:text="SAVE" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>